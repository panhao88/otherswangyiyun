.content{
  display: flex;
  justify-content: center;
  font-size: 32rpx;
}
.content_c{
  width: 94%;
}
.sheet_b{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 40rpx;
}
.sheet_c{
  font-weight: bold;
}
.sheet_d{
  width: 170rpx;
  height: 60rpx;
  line-height: 60rpx;
  border: 1px solid #eeeeee;
  border-radius: 40rpx;
  text-align: center;
  font-size: 30rpx;
}
.sheet_e{
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  margin-top: 36rpx;
}
.sheet_f{
  width: 31%;
  margin-bottom: 30rpx;
}
.sheet_g image{
  width: 100%;
  height: 220rpx;
  border-radius: 18rpx;
}
.sheet_g{
  position: relative;
}
.sheet_z{
  position: absolute;
  left: 30rpx;
  bottom: 20rpx;
  font-size: 24rpx;
  color: white;
  z-index: 2;
}
.sheet_h{
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  color: #666666;
  font-size: 30rpx;
  margin-top: 10rpx;
}
.sheet_y{
  display: flex;
  justify-content: space-between;
}
.sheet_x{
  width: 64%;
  line-height: 58rpx;
  display: flex;
  align-items: center;
}
.sheet_w{
  color: #666666;
  white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    word-break: break-all;
}
.sheet_11{
  margin-bottom: 30rpx;
}