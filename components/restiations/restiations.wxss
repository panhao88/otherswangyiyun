.top {
  width: 100%;
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}
.t {
  width: 95%;
  height: 80rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.n-top {
  display: flex;
  align-items: flex-end;
}
.bg {
  width: 150rpx;
  height: 50rpx;
  border-radius: 25rpx;
  font-size: 14px;
  border: 1px solid #eeeeee;
  display: flex;
  justify-content: center;
  align-items: center;
}
.nav {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.img {
  width: 100%;
  height: 220rpx;
  border-radius: 15rpx;
}
.imhg {
  width: 30%;
  margin-top: 10rpx;
  position: relative;
}
.font {
  font-size: 14px;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.newsong {
  margin-right: 10rpx;
}
.active {
  font-weight: 700;
  font-size: 18px;
}